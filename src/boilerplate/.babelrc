{
  "presets": [
    "react",
    "es2015",
    "es2016",
  ],
  "plugins": [
    ["transform-runtime", {
      "polyfill": true,
      "regenerator": false,
    }],
    "syntax-trailing-function-commas",
    "transform-object-rest-spread",
    "transform-class-properties",
    "transform-decorators-legacy",
    "syntax-async-functions",
    "async-to-promises",
  ]
}
