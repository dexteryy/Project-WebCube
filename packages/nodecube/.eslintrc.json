{
  "extends": [
    "webcube"
  ],
  "env": {
    "commonjs": false,
    "browser": false,
    "node": true
  },
  "settings": {
    "import/resolver": "node"
  },
  "globals": {
    "logger": true
  },
  "rules": {
    "prettier/prettier": [
      2,
      {
        "printWidth": 80,
        "tabWidth": 2,
        "useTabs": false,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "jsxBracketSameLine": true
      }
    ],
    "node/no-unsupported-features": 2,
    "filenames/match-regex": 0,
    "import/default": 0,
    "import/no-dynamic-require": 0,
    "import/no-commonjs": 0
  }
}
